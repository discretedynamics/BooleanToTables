{
   	"manifest_version": "2.0",
	"algo_name": "Local
ModelBooleanToTables", 
	"algo_version": "0.0.1",
	"algo_summary": "Python script to transform a local model in Boolean function form to transition table form ",
	"algo_description": "This script takes as input a local model, represented as a set of Boolean functions and formatted in the discretedynamics::localModelBoolean format. The script transforms the representation to transition tables, formatted in the discretedynamics::localModelTables format.", 
	"algo_website": "https://github.com/discretedynamics/localmodelconversions/booleantotables
", 
    "algo_keywords": ["local model","conversion"], 
	"published_paper": { 
            "title": "",
            "url": ""
    	},
	"algo_authors": [ 
		{
			"name": "Claus Kadelka",
			"email": "ckadelka@iastate.edu", 
		        "profile_picture": "",
			"personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
			"organization": "Iowa State University", 
			"org_website": "https://math.iastate.edu" 
		}
	],
	"algo_packager": { 
	    "name": "Claus Kadelka",
	    "email": "ckadelka@iastate.edu", 
            "profile_picture": "", 
	    "personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
	    "organization": "Iowa State University", 
	    "org_website": "https://math.iastate.edu" 
	},
	"algo_exec": "python3 BooleanToTables.py $localModelBoolean", 
	"algo_input": [
	    { "name": "localModelBoolean", "src": "file", "type": "text/plain", "accepted_format": "algorun/undefined" }
	],
	"algo_output": [ 
		{ "name": "localModelTables", "src": "localModelTables.txt", "type": "text/plain", "format": "discretedynamics/localModelTables" } 
	],
	"algo_parameters": { 
		"caching": "on" 
	},
	"algo_image": "discretedynamics/localmodelconversions/booleantotables"
}
