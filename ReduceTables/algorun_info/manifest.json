{
   	"manifest_version": "2.0",
	"algo_name": "reduceTables", 
	"algo_version": "0.0.1",
	"algo_summary": "Python script to delete non-essential variables from Tables",
	"algo_description": "This script takes as input a local model, represented as a set of transition tables, formatted in the discretedynamics::localModelTables format. For each table, it determines the non-essential variables and deletes them from the table. The final output is again a local model (in localModelTables format).", 
	"algo_website": "https://github.com/discretedynamics/localmodelconversions/reducetables
", 
    "algo_keywords": ["local model","conversion", "reduction"], 
	"published_paper": { 
            "title": "",
            "url": ""
    	},
	"algo_authors": [ 
		{
			"name": "Claus Kadelka",
			"email": "ckadelka@iastate.edu", 
		        "profile_picture": "",
			"personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
			"organization": "Iowa State University", 
			"org_website": "https://math.iastate.edu" 
		}
	],
	"algo_packager": { 
	    "name": "Claus Kadelka",
	    "email": "ckadelka@iastate.edu", 
            "profile_picture": "", 
	    "personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
	    "organization": "Iowa State University", 
	    "org_website": "https://math.iastate.edu" 
	},
	"algo_exec": "python3 ReduceTables.py $localModelTables",
	"algo_input": [
	    { "name": "localModelTables", "src": "file", "type": "text/plain", "accepted_format": "algorun/undefined" }
	],
	"algo_output": [
	    { "name": "reducedTable", "src": "reducedTable.txt", "type": "text/plain", "format": "algorun/undefined" }
	],
	"algo_parameters": { 
		"caching": "on" 
	},
	"algo_image": "discretedynamics/localmodelconversions/reducetables"
}
