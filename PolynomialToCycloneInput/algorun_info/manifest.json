{
   	"manifest_version": "2.0",
	"algo_name": "PolynomialsToCycloneInput", 
	"algo_version": "0.0.1",
	"algo_summary": "Python script to convert localModelPolynomial.json to Cyclone input",
	"algo_description": "This script takes a local model, represented as a set of polynomials, formatted in the discretedynamics::localModelPolynomials format. It transforms it into the format required by the old CYCLONE module on Algorun.", 
	"algo_website": "https://github.com/discretedynamics/localmodelconversions/PolynomialToCycloneInput
", 
    "algo_keywords": ["local model","conversion"], 
	"published_paper": { 
            "title": "",
            "url": ""
    	},
	"algo_authors": [ 
		{
			"name": "Claus Kadelka",
			"email": "ckadelka@iastate.edu", 
		        "profile_picture": "",
			"personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
			"organization": "Iowa State University", 
			"org_website": "https://math.iastate.edu" 
		}
	],
	"algo_packager": { 
	    "name": "Claus Kadelka",
	    "email": "ckadelka@iastate.edu", 
            "profile_picture": "", 
	    "personal_website": "https://math.iastate.edu/directory/claus-kadelka/",
	    "organization": "Iowa State University", 
	    "org_website": "https://math.iastate.edu" 
	},
	"algo_exec": "python3 LocalModelPolynomials_to_CycloneInput.py $localModelPolynomials",
	"algo_input": [
	    { "name": "localModelPolynomials", "src": "file", "type": "text/plain", "accepted_format": "algorun/undefined" }
	],
	"algo_output": [
	    { "name": "output", "src": "output.txt", "type": "text/plain", "format": "algorun/undefined" }
	],
	"algo_parameters": { 
		"caching": "on" 
	},
	"algo_image": "discretedynamics/localmodelconversions/PolynomialToCycloneInput"
}
